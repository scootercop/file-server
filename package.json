    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@types/express": "^4.17.12",
    "@types/http-server": "^0.10.1",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.0",
