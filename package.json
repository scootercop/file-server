{
  "name": "file-server",
  "version": "1.0.0",
  "description": "File sharing server with neat UI",
  "main": "server.js",
  "scripts": {
    "build:dev": "cross-env AppSettings=development webpack --config './webpack.dev.js'",
    "build:prod": "cross-env AppSettings=production webpack --config './webpack.prod.js'",
    "buildServer:noop": "cross-env AppSettings=production webpack --config ./webpack.config.js",
    "buildServer": "cd server && tsc && cd .."
  },
  "keywords": [
    "file",
    "server",
    "UI",
    "react"
  ],
  "author": "Naval Rishi",
  "license": "ISC",
  "dependencies": {
    "@fluentui/react": "^8.17.4",
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "mobx": "5.11.0",
    "mobx-react": "5.4.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@size-limit/preset-app": "^4.12.0",
    "@size-limit/webpack": "^4.12.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.12",
    "@types/http-server": "^0.10.1",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^8.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "shebang-loader": "0.0.1",
    "size-limit": "^4.12.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  },
  "size-limit": [
    {
      "path": "dist/src/index.js"
    }
  ]
}
